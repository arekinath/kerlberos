{deps, [
	{pbkdf2, "2.2.0", {pkg, mc_pbkdf2}},
	{neotoma, "1.7.3"}
]}.
{plugins, [
	rebar3_hex,
	{rebar3_neotoma_plugin, "0.2.0"},
	{provider_asn1, "0.2.3"}
]}.
{provider_hooks, [{pre, [
		    {compile, {asn, compile}},
		    {compile, {neotoma, compile}}
		  ]},
                  {post, [{clean, {asn, clean}}]}]}.
{asn1_args, [{encoding, ber}, {compile_opts, [ber, der, undec_rest]}]}.

{shell, [{apps, [kerlberos]}]}.
